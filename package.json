{
  "name": "js-library-template",
  "version": "2.0.4",
  "repository": "https://github.com/dzek69/js-library-template.git",
  "author": "Jacek Nowacki @dzek69 <git-public@dzek.eu>",
  "license": "MIT",
  "bin": {
    "jslib": "cli.mjs"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=testing mocha --require ./test/bootstrap.cjs 'src/**/*.spec.mjs'",
    "docs": "node build-scripts/docs.mjs && jsdoc -r src README.md -t node_modules/docdash -d ./docs -u ./tutorials -c jsdoc.json && node build-scripts/docs.after.mjs",
    "transpile": "node build-scripts/transpile.mjs && babel src -d dist --ignore **/*.spec.mjs",
    "prepublishOnly": "yarn lint && yarn test && yarn docs",
    "prepack": "yarn transpile",
    "lint": "cross-env eslint --report-unused-disable-directives '*.mjs' 'src/**/*.mjs' 'src/*.mjs'",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "bottom-line-utils": "^0.6.3",
    "empty-dir": "^2.0.0",
    "fs-extra": "^8.1.0",
    "semver": "^7.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "@dzek69/eslint-config-base": "^1.0.1",
    "babel-plugin-module-extension": "^0.1.1",
    "babel-plugin-rewire": "^1.2.0",
    "cross-env": "^7.0.2",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.2",
    "must": "^0.13.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepublishOnly && yarn transpile"
    }
  }
}
